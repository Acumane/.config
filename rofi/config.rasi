@theme "adwaita"

configuration {
	show-icons: false;
	steal-focus: true;

	display-drun: "RUN";
	display-clipboard: "COPY";
	display-filebrowser: "files";
	drun-display-format: "{name}";
	font: "Akkurat 11";
	
	filebrowser {
	/*	directory: "/home";*/
		directories-first: true;
		sorting-method: "name";
	}
}
