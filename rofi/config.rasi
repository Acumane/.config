@theme "adwaita"

configuration {
	show-icons: false;
	hover-select: true;
	/* steal-focus: true; */

	display-drun: "RUN";
	display-clipboard: "COPY";
	display-filebrowser: "files";
	drun-display-format: "{name}";
	font: "Akkurat 11";
	eh: 1;
	/* dpi: 92; */
	yoffset: 20;
	/* xoffset: 750; */
	
	filebrowser {
	/*	directory: "/home";*/
		directories-first: true;
		sorting-method: "name";
	}
}
